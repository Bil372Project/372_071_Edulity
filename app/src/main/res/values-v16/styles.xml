<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ColumnHeading" parent="android:Widget.TextView">
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textColor">#293a80</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">18dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:padding">5dp</item>
        <item name="android:layout_weight">20</item>
        <item name="android:gravity">center</item>
        <item name="android:fontFamily">@font/anton</item>
        <item name="android:layout_marginBottom">5dp</item>

    </style>
    <style name="ColumnHeading.TableCell">
        <item name="android:text">textview</item>
        <item name="android:height">50dp</item>
        <item name="android:textSize"> 12dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#8186d5</item>
        <item name="android:background">#eeeeee</item>
    </style>

    <style name="ColumnHeading.TableCell.TableStartTime">
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:background">#6ed</item>
        <item name="android:textColor">#aaaaaa</item>
    </style>
</resources>